266: \n267:                 {/* Retrieval Top-K */}\n268:                 <div className=\"space-y-2\">\n269:                   <label className=\"text-sm font-medium\">Retrieval Top-K</label>\n270:                   <Input\n271:                     type=\"number\"\n272:                     min={1}\n273:                     value={formData.max_context_chunks}\n274:                     onChange={(e) => handleInputChange(\"max_context_chunks\", e.target.value)}\n275:                     className={errors.max_context_chunks ? \"border-destructive\" : \"\"}\n276:                   />\n277:                   {errors.max_context_chunks && (\n278:                     <p className=\"text-xs text-destructive\">{errors.max_context_chunks}</p>\n279:                   )}\n280:                   <p className=\"text-xs text-muted-foreground\">\n281:                     Maximum number of chunks to retrieve and include in context\n282:                   </p>\n283:                 </div>\n284: \n285:                 {/* Max Distance Threshold */}\n286:                 <div className=\"space-y-2\">\n287:                   <label className=\"text-sm font-medium\">Max Distance Threshold</label>\n288:                   <div className=\"flex items-center gap-4\">\n289:                     <Input\n290:                       type=\"number\"\n291:                       min={0}\n292:                       max={1}\n293:                       step={0.01}\n294:                       value={formData.rag_relevance_threshold}\n295:                       onChange={(e) => handleInputChange(\"rag_relevance_threshold\", e.target.value)}\n296:                       className={`w-24 ${errors.rag_relevance_threshold ? \"border-destructive\" : \"\"}`}\n297:                     />\n298:                     <input\n299:                       type=\"range\"\n300:                       min={0}\n301:                       max={1}\n302:                       step={0.01}\n303:                       value={formData.rag_relevance_threshold}\n304:                       onChange={(e) => handleInputChange(\"rag_relevance_threshold\", e.target.value)}\n305:                       className=\"flex-1\"\n306:                     />\n307:                   </div>\n308:                   {errors.rag_relevance_threshold && (\n309:                     <p className=\"text-xs text-destructive\">{errors.rag_relevance_threshold}</p>\n310:                   )}\n311:                   <p className=\"text-xs text-muted-foreground\">\n312:                     Maximum distance (1-0) for chunks to be included in context (lower = more strict)\n313:                   </p>\n314:                 </div>\n315: \n316:                 {/* Auto Scan Enabled */}\n317:                 <div className=\"space-y-2\">\n318:                   <div className=\"flex items-center gap-2\">\n319:                     <input\n320:                       type=\"checkbox\"\n321:                       id=\"auto_scan_enabled\"\n322:                       checked={formData.auto_scan_enabled}\n323:                       onChange={(e) => handleInputChange(\"auto_scan_enabled\", e.target.checked)}\n324:                       className=\"h-4 w-4 rounded border-gray-300\"\n325:                     />\n326:                     <label htmlFor=\"auto_scan_enabled\" className=\"text-sm font-medium\">\n327:                       Enable Auto Scan\n328:                     </label>\n329:                   </div>\n330:                   <p className=\"text-xs text-muted-foreground\">\n331:                     Automatically scan for new documents at regular intervals\n332:                   </p>\n333:                 </div>\n334: \n335:                 {/* Auto Scan Interval */}\n336:                 {formData.auto_scan_enabled && (\n337:                   <div className=\"space-y-2\">\n338:                     <label className=\"text-sm font-medium\">Auto Scan Interval (minutes)</label>\n339:                     <Input\n340:                       type=\"number\"\n341:                       min={1}\n342:                       value={formData.auto_scan_interval_minutes}\n343:                       onChange={(e) => handleInputChange(\"auto_scan_interval_minutes\", e.target.value)}\n344:                       className={errors.auto_scan_interval_minutes ? \"border-destructive\" : \"\"}\n345:                     />\n346:                     {errors.auto_scan_interval_minutes && (\n347:                       <p className=\"text-xs text-destructive\">{errors.auto_scan_interval_minutes}</p>\n348:                     )}\n349:                     <p className=\"text-xs text-muted-foreground\">\n350:                       How often to scan for new documents (in minutes)\n351:                     </p>\n352:                   </div>\n353:                 )}\n354: \n355:                 {/* Retrieval Window */}\n356:                 <div className=\"space-y-2\">\n357:                   <label className=\"text-sm font-medium\">Retrieval Window</label>\n358:                   <Input\n359:                     type=\"number\"\n360:                     min={0}\n361:                     max={3}\n362:                     value={formData.retrieval_window}\n363:                     onChange={(e) => handleInputChange(\"retrieval_window\", e.target.value)}\n364:                     className={errors.retrieval_window ? \"border-destructive\" : \"\"}\n365:                   />\n366:                   {errors.retrieval_window && (\n367:                     <p className=\"text-xs text-destructive\">{errors.retrieval_window}</p>\n368:                   )}\n369:                   <p className=\"text-xs text-muted-foreground\">\n370:                     Number of adjacent chunks to include (0-3)\n371:                   </p>\n372:                 </div>\n373: \n374:                 {/* Vector Metric */}\n375:                 <div className=\"space-y-2\">\n376:                   <label className=\"text-sm font-medium\">Vector Metric</label>\n377:                   <select\n378:                     value={formData.vector_metric}\n379:                     onChange={(e) => handleInputChange(\"vector_metric\", e.target.value)}\n380:                     className={`w-full rounded-md border border-input bg-background px-3 py-2 text-sm ring-offset-background file:border-0 file:bg-transparent file:text-sm file:font-medium placeholder:text-muted-foreground focus-visible:outline-none focus-visible:ring-2 focus-visible:ring-ring focus-visible:ring-offset-2 disabled:cursor-not-allowed disabled:opacity-50 ${errors.vector_metric ? \"border-destructive\" : \"\"}`}\n381:                   >\n382:                     <option value=\"cosine\">Cosine Similarity</option>\n383:                     <option value=\"euclidean\">Euclidean Distance</option>\n384:                     <option value=\"dot_product\">Dot Product</option>\n385:                   </select>\n386:                   {errors.vector_metric && (\n387:                     <p className=\"text-xs text-destructive\">{errors.vector_metric}</p>\n388:                   )}\n389:                   <p className=\"text-xs text-muted-foreground\">\n390:                     Distance metric used for vector similarity search\n391:                   </p>\n392:                 </div>\n393: \n394:                 {/* Save Button and Status */}\n395:                 <div className=\"flex items-center gap-4 pt-4 border-t\">\n396:                   <Button\n397:                     onClick={handleSave}\n398:                     disabled={saving || !hasChanges()}\n399:                   >\n400:                     {saving && <Loader2 className=\"w-4 h-4 mr-2 animate-spin\" />}\n401:                     Save Changes\n402:                   </Button>\n403: \n404:                   {hasChanges() && (\n405:                     <span className=\"text-sm text-muted-foreground\">You have unsaved changes</span>\n406:                   )}\n407:                 </div>\n408:               </CardContent>\n409:             </Card>\n410: \n411:             {/* Advanced Embedding Settings */}\n412:             <Card>\n413:               <CardHeader>\n414:                 <CardTitle>Advanced Embedding Settings</CardTitle>\n415:                 <CardDescription>Configure embedding prefixes for different content types</CardDescription>\n416:               </CardHeader>\n417:               <CardContent className=\"space-y-6\">\n418:                 <div className=\"space-y-2\">\n419:                   <label className=\"text-sm font-medium\">Embedding Document Prefix</label>\n420:                   <textarea\n421:                     value={formData.embedding_doc_prefix}\n422:                     onChange={(e) => handleInputChange(\"embedding_doc_prefix\", e.target.value)}\n423:                     className={`w-full rounded-md border border-input bg-background px-3 py-2 text-sm ring-offset-background placeholder:text-muted-foreground focus-visible:outline-none focus-visible:ring-2 focus-visible:ring-ring focus-visible:ring-offset-2 disabled:cursor-not-allowed disabled:opacity-50 min-h-[80px] ${errors.embedding_doc_prefix ? \"border-destructive\" : \"\"}`}\n424:                     placeholder=\"Passage: \"\n425:                   />\n426:                   {errors.embedding_doc_prefix && (\n427:                     <p className=\"text-xs text-destructive\">{errors.embedding_doc_prefix}</p>\n428:                   )}\n429:                   <p className=\"text-xs text-muted-foreground\">\n430:                     Prefix added to document chunks before embedding (default: \"Passage: \")\n431:                   </p>\n432:                 </div>\n433: \n434:                 <div className=\"space-y-2\">\n435:                   <label className=\"text-sm font-medium\">Embedding Query Prefix</label>\n436:                   <textarea\n437:                     value={formData.embedding_query_prefix}\n438:                     onChange={(e) => handleInputChange(\"embedding_query_prefix\", e.target.value)}\n439:                     className={`w-full rounded-md border border-input bg-background px-3 py-2 text-sm ring-offset-background placeholder:text-muted-foreground focus-visible:outline-none focus-visible:ring-2 focus-visible:ring-ring focus-visible:ring-offset-2 disabled:cursor-not-allowed disabled:opacity-50 min-h-[80px] ${errors.embedding_query_prefix ? \"border-destructive\" : \"\"}`}\n440:                     placeholder=\"Query: \"\n441:                   />\n442:                   {errors.embedding_query_prefix && (\n443:                     <p className=\"text-xs text-destructive\">{errors.embedding_query_prefix}</p>\n444:                   )}\n445:                   <p className=\"text-xs text-muted-foreground\">\n446:                     Prefix added to queries before embedding (default: \"Query: \")\n447:                   </p>\n448:                 </div>\n449:               </CardContent>\n450:             </Card>\n451:           </TabsContent>\n452: \n453:           <TabsContent value=\"api-key\">\n454:             <Card>\n455:               <CardHeader>\n456:                 <CardTitle>API Key</CardTitle>\n457:                 <CardDescription>Configure API key for authenticated requests</CardDescription>\n458:               </CardHeader>\n459:               <CardContent className=\"space-y-4\">\n460:                 <div className=\"space-y-2\">\n461:                   <label className=\"text-sm font-medium\">API Key</label>\n462:                   <Input\n463:                     type=\"password\"\n464:                     value={apiKey}\n465:                     onChange={(e) => setApiKey(e.target.value)}\n466:                     placeholder=\"Enter your API key (if configured)\"\n467:                   />\n468:                   <p className=\"text-xs text-muted-foreground\">\n469:                     Optional. Set this if your server requires authentication. The key is stored in your browser's localStorage.\n470:                   </p>\n471:                 </div>\n472:                 <div className=\"flex items-center gap-4\">\n473:                   <Button onClick={handleApiKeySave}>\n474:                     Save API Key\n475:                   </Button>\n476:                   {apiKeySaved && (\n477:                     <span className=\"text-sm text-green-600 dark:text-green-400\">Saved</span>\n478:                   )}\n479:                 </div>\n480:               </CardContent>\n481:             </Card>\n482:           </TabsContent>\n483:         </Tabs>\n484:       )}\n485:     </>\n486:   );\n487: }\n488: \n489: // Wrapper that provides health status and connection test\n490: function SettingsPageWithStatus({ health }: { health: HealthStatus }) {\n491:   const formatLastChecked = (date: Date | null) => {\n492:     if (!date) return \"Not checked\";\n493:     return `Last checked: ${date.toLocaleTimeString()}`;\n494:   };\n495: \n496:   const [connectionResult, setConnectionResult] = useState<ConnectionTestResult | null>(null);\n497:   const [isTestingConnections, setIsTestingConnections] = useState(false);\n498: \n499:   const handleConnectionTest = async () => {\n500:     setIsTestingConnections(true);\n501:     try {\n502:       const result = await testConnections();\n503:       setConnectionResult(result);\n504:       toast.success(\"Connection test completed\");\n505:     } catch (err) {\n506:       const message = err instanceof Error ? err.message : \"Connection test failed\";\n507:       toast.error(message);\n508:       setConnectionResult(null);\n509:     } finally {\n510:       setIsTestingConnections(false);\n511:     }\n512:   };\n513: \n514:   return (\n515:     <div className=\"space-y-6 animate-in fade-in duration-300\">\n516:       <div className=\"flex items-center justify-between\">\n517:         <div>\n518:           <h1 className=\"text-3xl font-bold tracking-tight\">Settings</h1>\n519:           <p className=\"text-muted-foreground mt-1\">Configure your application preferences</p>\n520:         </div>\n521:         <div className=\"flex flex-col items-end gap-1\">\n522:           <ConnectionStatusBadges health={health} />\n523:           <span className=\"text-xs text-muted-foreground\">{formatLastChecked(health.lastChecked)}</span>\n524:           <Button size=\"sm\" variant=\"outline\" onClick={handleConnectionTest} disabled={isTestingConnections}>\n525:             {isTestingConnections ? (\n526:               <Loader2 className=\"w-3 h-3 animate-spin\" />\n527:             ) : (\n528:               \"Test Connections\"\n529:             )}\n530:           </Button>\n531:           {connectionResult && (\n532:             <div className=\"flex gap-2\">\n533:               {Object.entries(connectionResult).map(([service, info]) => (\n534:                 <Badge key={service} variant={info.ok ? \"outline\" : \"destructive\"} className=\"text-xs\">\n535:                   {service}: {info.ok ? \"OK\" : \"Fail\"}\n536:                 </Badge>\n537:               ))}\n538:             </div>\n539:           )}\n540:         </div>\n541:       </div>\n542:       <SettingsPageContent />\n543:     </div>\n544:   );\n545: }\n546: \n547: // Main SettingsPage that checks health\n548: export default function SettingsPage() {\n549:   const health = useHealthCheck();\n550:   return <SettingsPageWithStatus health={health} />;\n551: }\n\n(End of file - total 551 lines)\n</content>","timestamp":1771874473116,"originalBytes":26092}